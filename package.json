{
  "name": "selecao-aluno",
  "version": "0.0.0",
  "description": "API responsavel por fornecer dados referentes ao processo seletivo para cursos oferecidos pelo Estado.",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generator": "typeorm-model-generator -h localhost -d selecao-aluno -p 5432 -u postgres -x 123456 -e postgres"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/platform-express": "^6.1.1",
    "@nestjs/swagger": "^3.0.2",
    "@types/apicache": "^1.2.0",
    "@types/redis": "^2.8.12",
    "@types/redis-mock": "^0.17.0",
    "apicache": "^1.4.0",
    "apollo-server-express": "^2.14.2",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "install": "^0.12.2",
    "jest-cucumber": "^2.0.11",
    "mssql": "^5.1.0",
    "pg": "^7.9.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "swagger-ui-express": "^4.0.2",
    "typeorm": "^0.2.16",
    "typeorm-model-generator": "^0.3.4",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^29.3.1",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "redis-mock": "^0.43.0",
    "standard-version": "^8.0.1",
    "supertest": "^3.4.2",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
